{
    "name": "romainj/drupal-base-profile",
    "description": "Drupal base profile.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/romainj/drupal-base-profile",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "drupal/admin_toolbar": "*",
        "drupal/advagg": "*",
        "drupal/back_office_access_restriction": "*",
        "drupal/bootstrap_layout_builder": "*",
        "drupal/chosen": "*",
        "drupal/config_ignore": "*",
        "drupal/config_split": "*",
        "drupal/contact_storage": "*",
        "drupal/entity_usage": "*",
        "drupal/eu_cookie_compliance": "*",
        "drupal/layout_builder_blocks": "*",
        "drupal/layout_builder_modal": "*",
        "drupal/layout_builder_restrictions": "*",
        "drupal/metatag": "*",
        "drupal/paragraphs": "*",
        "drupal/password_policy": "*",
        "drupal/pathauto": "*",
        "drupal/recaptcha": "*",
        "drupal/redirect": "*",
        "drupal/seckit": "*",
        "drupal/stage_file_proxy": "*",
        "drupal/verf": "*",
        "drupal/views_infinite_scroll": "*"
    },
    "require-dev": {
        "drupal/devel": "*",
        "drupal/reroute_email": "*",
        "kint-php/kint": "*"
    }
}
